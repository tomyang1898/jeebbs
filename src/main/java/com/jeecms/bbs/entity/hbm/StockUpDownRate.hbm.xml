<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.jeecms.bbs.entity">
	<class name="StockUpDownRate" table="stock_updown_rate">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="int" column="id"><generator class="native"/></id>
		<property name="qiangRuoRate" column="qiangRuoRate" type="double" />
		<property name="upAndDownRate" column="upAndDownRate" type="double" />
		<property name="date" column="date" type="date"  />		
	</class>
</hibernate-mapping>